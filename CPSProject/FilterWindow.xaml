<Window x:Class="CPSProject.FilterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CPSProject.Controller"
        mc:Ignorable="d"
        Title="Filtrowanie" Height="150" Width="306"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Rząd filtru:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Częstotliwość odcięcia:" HorizontalAlignment="Left" Margin="10,44,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="182,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="90" Name="filterDegree"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="182,47,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="90" Name="cutoffFrequency"/>
        <Button Content="Filtruj" HorizontalAlignment="Left" Margin="18,85,0,0" VerticalAlignment="Top" Width="259" Click="OkButton"/>
    </Grid>
</Window>
