<Window x:Class="CPSProject.QuantizeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CPSProject.Controller"
        mc:Ignorable="d"
        Title="Kwantyzacja" Height="152" Width="306"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Częstotliwość próbkowania:" HorizontalAlignment="Left" Margin="13,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Liczba poziomów kwantyzacji:" HorizontalAlignment="Left" Margin="10,44,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="182,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="90" Name="Sampling"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="182,47,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="90" Name="Levels"/>
        <Button Content="Kwantyzuj" HorizontalAlignment="Left" Margin="13,82,0,0" VerticalAlignment="Top" Width="259" Click="OkButton"/>
    </Grid>
</Window>
